{
    "name": "atmospherephp/framework",
    "description": "Atmosphere framework for PHP.",
    "keywords": [
        "framework",
        "php",
        "php-framework",
        "laravel-alternative",
        "atmosphere",
        "atmospherephp",
        "atmosphere-framework"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Romano Schoonheim",
            "email": "support@atmosphere.sh"
        }
    ],
    "require": {
        "symfony/yaml": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Atmospherephp\\Framework\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Atmospherephp\\Framework\\Tests\\": "tests/"
        }
    }
}
